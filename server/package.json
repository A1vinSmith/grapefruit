{
  "devDependencies": {
    "@types/chai": "^4.2.11",
    "@types/chai-as-promised": "^7.1.2",
    "@types/koa": "^2.11.3",
    "@types/koa-bodyparser": "^4.3.0",
    "@types/koa-router": "^7.4.0",
    "@types/mocha": "^7.0.2",
    "@types/node": "^13.13.36",
    "@types/plist": "^3.0.2",
    "@types/socket.io": "^2.1.4",
    "@typescript-eslint/eslint-plugin": "^2.25.0",
    "@typescript-eslint/parser": "^2.25.0",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "cross-env": "^7.0.2",
    "eslint": "^6.8.0",
    "eslint-plugin-import": "^2.20.1",
    "mocha": "^7.1.1",
    "nodemon": "^2.0.4",
    "standard": "^14.3.3",
    "ts-node": "^8.8.1",
    "typescript": "^3.8.3"
  },
  "private": "true",
  "name": "grapefruit-server",
  "main": "dist/app.js",
  "directories": {
    "lib": "lib",
    "test": "test"
  },
  "dependencies": {},
  "scripts": {
    "postinstall": "npx ts-node scripts/migrate",
    "test": "mocha --expose-gc -r ts-node/register tests/**/*.ts",
    "test2": "mocha --expose-gc -r ts-node/register",
    "build": "tsc",
    "dev": "nodemon",
    "lint": "eslint app.ts lib/**.ts"
  },
  "nodemonConfig": {
    "ignore": [
      "**/*.test.ts",
      "**/*.spec.ts",
      ".git",
      "node_modules"
    ],
    "watch": [
      "lib",
      "app.ts"
    ],
    "exec": "cross-env NODE_ENV=development node -r ts-node/register app.ts",
    "ext": "ts"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "iOS",
    "mobile",
    "security",
    "instruments",
    "reverse",
    "engineering"
  ],
  "author": "CodeColorist",
  "license": "MIT"
}
